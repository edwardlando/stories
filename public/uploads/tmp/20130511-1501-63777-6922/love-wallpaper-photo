	<div class="text16" align="center" style="margin:0 0 4px 0"><b><a href="/clubs/love/quiz/show/740283/love-wallpaper-photo" style="color:#000000;text-decoration:none">Spanish version of &#34;I love you&#34; [2] is...</a></b></div>
	<div class="clear"></div><div align="center"><img src="http://images5.fanpop.com/image/quiz/740000/740283_1322194986775_300_400.png" width="150" height="200" alt="Spanish version of &#34;I love you&#34; [2] is..." class="border" style="margin:2px 0 4px 0" /></div><div class="clear"></div>
	

	<script type="text/javascript" language="javascript">
	// <![CDATA[
		function quizhighlight(num)
		{
			background_color = "#F0F8F0";
			if (document.getElementById("quiz_option_"+num).style.backgroundColor == "transparent")
			{
				document.getElementById("quiz_option_"+num).style.backgroundColor = background_color;
				//$("quiz_answer_bubble_"+num).src = "/images/quiz/answered"+Math.floor(Math.random()*5)+".jpg";
				document.getElementById("quiz_answer_bubble_"+num).src = "/images/quiz/answered4.jpg";
			}
			
		}
		function quizunhighlight(num)
		{
			document.getElementById("quiz_option_"+num).style.backgroundColor = "transparent";
			document.getElementById("quiz_answer_bubble_"+num).src = "/images/quiz/answer"+(num+1)+".jpg";
		}
		function submitvote(url)
		{
			document.getElementById("quizchoices").innerHTML = "<div style='text-align:center;padding:12px 0 12px 0;font-size:18px;font-weight:bold'>Submitting...</div>";
			window.location=url;
		}
	// ]]>
	</script>
